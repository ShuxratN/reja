                    </div>
                    <div class="info-row">
                        <span class="label">Residence:</span>
                        <div class="left_2-2"><%=user.residence%></div>
                    </div>
                    <div class="info-row">
                        <span class="label">City:</span> 
                        <div class="left_2-3"><%=user.city%></div>
                    </div>
                    <div class="info-row">
                        <span class="label">Address:</span> 
                        <div class="left_2-4"><%=user.adress%></div>
                    </div>
                </div>
                
                </div>
                <div class="marginer"></div>
                <div class="left_3">
                    
                    <div class="container">
                        
                            <!-- Languages Section -->
                            <div class="languages">
                              <h3>Languages</h3>
                              <!-- Display the first language -->
                              <div class="language-item">
                                <span class="language-name"><%= user.languages[0]%></span>
                                <div class="chiz"> <img src="icon.sariqchiz.png" alt=""></div>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.languages[0].percent %>%;"></div>
                                </div>
                                <span class="language-percent"><%= user.languages[0].percent %>82%</span>
                              </div>
                              <!-- Display the second language -->
                              <div class="language-item">
                                <span class="language-name"><%= user.languages[1] %></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.languages[1].percent %>%;"></div>
                                </div>
                                <span class="language-percent"><%= user.languages[1].percent %>70%</span>
                              </div>
                              <!-- Display the third language -->
                              <div class="language-item">
                                <span class="language-name"><%= user.languages[2] %></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.languages[2].percent %>%;"></div>
                                </div>
                                <span class="language-percent"><%= user.languages[2].percent %>70%</span>
                              </div>
                            </div>
                            <div class="container">
                            <!-- Skills Section -->
                            <div class="skills">
                              <h3>Skills</h3>
                              <!-- Display the first skill -->
                              <div class="skill-item">
                                <span class="skill-name"><%= user.skills[0]%></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.skills[0].percent %>%;"></div>
                                </div>
                                <span class="skill-percent"><%= user.skills[0].percent %>%</span>
                              </div>
                              <!-- Display the second skill -->
                              <div class="skill-item">
                                <span class="skill-name"><%= user.skills[1]%></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.skills[1].percent %>%;"></div>
                                </div>
                                <span class="skill-percent"><%= user.skills[1].percent %>%</span>
                              </div>
                              <!-- Display the third skill -->
                              <div class="skill-item">
                                <span class="skill-name"><%= user.skills[2]%></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.skills[2].percent %>%;"></div>
                                </div>
                                <span class="skill-percent"><%= user.skills[2].percent %>%</span>
                              </div>
                              <div class="skill-item">
                                <span class="skill-name"><%= user.skills[3]%></span>
                                <div class="progress-bar">
                                  <div class="progress-fill" style="width: <%= user.skills[2].percent %>%;"></div>
                                </div>
                                <span class="skill-percent"><%= user.skills[3].percent %>%</span>
                              </div>
                            </div>
                          </div>
                        

                      </div>
