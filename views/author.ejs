                    <div class="righticon"> <img src="icon/right3.svg"></div>
                    <div class="righticon"> <img src="icon/right4.svg"></div>
                    <div class="righticon"> <img src="icon/right5.svg"></div>
                    <div class="righticon"> <img src="icon/right6.svg"></div>
